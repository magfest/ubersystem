[dept_head_checklist]
[[placeholders]]
deadline = 2023-10-19
name = Checking Placeholder Registrations
description = '''We create placeholder registrations for volunteers and ask them to fill out the rest of their information and also confirm that they'll be coming. We need our department heads to review the unclaimed badges for their departments to check for any essential volunteers who haven't claimed their badges.
'''
path = /dept_checklist/placeholders?department_id={department_id}
[[creating_shifts]]
deadline = 2023-11-02
description = '''We are able to import shifts from last year. If you want us to import last year's shifts, please  email stops@magfest.org. You will still need to approve these via the checklist after the import.
'''
path = /shifts_admin/index?department_id={department_id}
[[assigned_volunteers]]
deadline = 2023-10-19
name = Volunteers Assigned to Your Department
description = '''Check all of the volunteers currently assigned to your department to make sure no one is missing AND that no one is there who shouldn't be.
'''
path = /shifts_admin/staffers?department_id={department_id}
[[treasury]]
deadline = 2023-12-14
name = Treasury Information
description = "If you need cash and/or mpoints, tell us your department schedule and your specific cash needs."
path = /dept_checklist/treasury?department_id={department_id}
[[hotel_setup]]
deadline = 2023-11-09
name = Hotel Setup Information
description = "Do you need tables, chairs, trash cans, water dispensers, etc?"
external_form_url = https://docs.google.com/forms/d/e/1FAIpQLSd3Yvmso5btjIHUp0KQdj_G6N9UnuzmgvsXM5h0Fo6EanqiRg/viewform
[[approve_setup_teardown]]
deadline = 2023-11-16
name = Approve/Decline Additional Hotel Nights
description = '''Many staffers want to work setup and teardown shifts rather than work during the event. We have more  volunteers than we need to do this, so we require DH approval of volunteers who need hotel space for  those dates.
'''
path = /dept_checklist/hotel_requests?department_id={department_id}
[[tech_requirements]]
deadline = 2023-11-09
name = Tech Requirements
description = "What do you need in terms of laptops, projectors, cables, internet access, etc?"
full_description = '''What do you need in terms of laptops, projectors, cables, internet access, etc? Note: Your department must fill out the form, even if you don't need anything. If you have any questions, please  contact TechOps on Slack ( #super-techops ) or via email ( techops@magfest.org )
'''
external_form_url = https://docs.google.com/forms/d/e/1FAIpQLSf7NMe4mJDHHc-EOTKmYACpb4Y4WgDlx6KwpiWQUJyYnX_Y9Q/viewform
[[printed_signs]]
deadline = 2023-11-16
name = Room Signage
description = "Other than a sign for your area, what printed coroplast signs or banners do you need?"
path = /dept_checklist/printed_signs?department_id={department_id}
full_description = '''Other than a sign for your area, what printed coroplast signs or banners do you need? Do you require any signage about COVID precautions or protocol changes?
'''
[[logistics]]
deadline = 2023-11-16
name = Logistics Needs
description = What do you need brought from the warehouse?
external_form_url = https://docs.google.com/forms/d/e/1FAIpQLScdIlQgivHX6gWb5_KNrY6keWkmAw4QvgwgwxQPi0jU2jUINQ/viewform
[[mediatron]]
deadline = 2023-11-30
name = Mediatron! Shot List
description = '''If you have something especially cool happening that you would like Mediatron! to capture, let us know  here so they can do their best to get your event on their schedule. For detailed or special requests,  please contact mediatron@magfest.org.
'''
external_form_url = https://forms.gle/FfjCPCmaRRQwj1B58
[[office_supplies]]
deadline = 2023-11-30
description = '''STOPS can provide the following office supplies: pens, sharpies, paper, whiteboard, whiteboard markers,  sticky notes, scissors, painters tape, scotch tape, staplers, clipboards. List  WHICH ITEMS you need and HOW MANY you need of each item (if you request an item but don't give a quantity,  you will get 1). If you need a very specific office item, you will need to purchase it yourself using your  department's budget. Note 1: STOPS has a paper cutter and a printer for small jobs (less than 50 pages).  Anything more than 50 pages needs to go into 'Bulk Print Jobs.' Note 2: A ream of paper is 500 sheets.
'''
external_form_url = https://forms.gle/dzhTkHsfrNUE9aya7
[[guidebook_schedule]]
deadline = 2023-11-30
name = Schedule for Guidebook
description = Make sure your public-facing events or open hours are on our Uber schedule so they can be exported to Guidebook.
path = /dept_checklist/guidebook_schedule?department_id={department_id}
[[social_media]]
deadline = 2023-12-07
name = Social Media Requests
description = '''If you have a guest or event that you believe is worth promoting on social media beforehand, let us know!  For detailed or special requests, please contact socialmedia@magfest.org. You can also make requests in  #socialmedia-requests on Slack.
'''
external_form_url = https://docs.google.com/forms/d/e/1FAIpQLSdas7NaPR_s4vllpcQL696nZ1qLq3yGRJuyKnvXn8MZPYP7Dw/viewform
[[bulk_print_jobs]]
deadline = 2023-12-07
description = '''Do you have any bulk print jobs (more than 50 pages) you'd like us to print in advance? STOPS has a paper  cutter and a printer for small jobs (less than 50 pages). Anything more than 50 pages should go here! For  EACH item you need printed, please include a link to the document and the number of copies.
'''
[[water_requests]]
deadline = 2023-12-07
description = '''Please let us know how much water or sports drinks you need ordered for your staff. Note: while Staff Suite  coordinates the ordering of these items, departments are responsible for picking up their own water and sports  drinks from the expo halls in a timely fashion. Water/drink orders will be adjusted at the discretion of Staff  Suite leadership if needed. 
'''
external_form_url = https://docs.google.com/forms/d/1txm6bho9Vzso7FdgpDS7dDkwdd9nbEFmhL5avfIo63Q/edit?usp=sharing
[[postcon_hours]]
deadline = 2024-01-18
name = (After the Event) Marking + Rating Shifts
description = '''Please go through your volunteers shifts carefully. This ensures that they receive the appropriate perks,  are imported as staff next year, maintain eligibility for hotel crash space, etc.
'''
path = /shifts_admin/signups?department_id={department_id}
email_post_con = True
[[ppe_requests]]
deadline = 2023-12-07
name = Bulk PPE & Sanitizing Supplies
description = '''MAGFest is providing basic PPE for departments for Super 2024. Please let us know what you need and we will do our best to accommodate.
'''
external_form_url = https://forms.gle/BihzhdCWXp7juwSC9
